doctype html
html
    head
        title Cargar Medico
        link(rel="stylesheet", href="/css/adminCargarProfesional.css")
    body
        header
            a(href="http://localhost:3000/admin/home", rel="noopener noreferrer"): img(src="/img/agenda.png", alt="" class="logoHeader")
            nav 
                ul(class="ulHeader")
                    li: a(href="/admin/home") Inicio
                    li: a(href="/admin/cargarProfesional") Medicos
                    li: a(href="/admin/cargarAgenda") Agendas
                    li: a(href="javascript:void(0);" id="logoutButton") Cerrar Sesi√≥n
        main 
            h1 Cargar Medicos
            section(class="sectionCrud")
                article(class="articleCrud") 
                    a(href="http://localhost:3000/admin/createProfesional", rel="noopener noreferrer"): img(src="/img/create.png", alt="logoCreate" class="logoCreate")
                    span Cargar un medico
                article(class="articleCrud") 
                    a(href="http://localhost:3000/admin/readProfesional", rel="noopener noreferrer"): img(src="/img/read.png", alt="logoRead" class="logoRead")
                    span Lista de medicos
                article(class="articleCrud") 
                    a(href="http://localhost:3000/admin/updateProfesional", rel="noopener noreferrer"): img(src="/img/update.png", alt="logoUpdate" class="logoUpdate")
                    span Editar datos de un medico
                article(class="articleCrud")
                    a(href="http://localhost:3000/admin/deleteProfesional", rel="noopener noreferrer"): img(src="/img/delete.png", alt="logoDelete" class="logoDelete")
                    span Borrar un medico
