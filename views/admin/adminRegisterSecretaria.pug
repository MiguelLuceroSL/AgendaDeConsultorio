doctype html
html
    head
        title Cargar Médico
        link(rel="stylesheet", href="/css/adminCargarProfesional.css")
        link(rel="stylesheet", href="/css/adminCreateProfesional.css")
        link(rel="stylesheet", href="/css/login.css")
    body
        header
            a(href="http://localhost:3000/admin/home", rel="noopener noreferrer"): img(src="/img/agenda.png", alt="" class="logoHeader")
            nav 
                ul(class="ulHeader")
                    li: a(href="/admin/home") Inicio
                    li: a(href="/admin/cargarProfesional") Médicos
                    li: a(href="/admin/registerSecretaria") Registrar secretaria
                    li: a(href="/admin/cargarAgenda") Agendas
                    li: a(href="javascript:void(0);" id="logoutButton") Cerrar Sesión
        section.seccion
            article.articuloRegistro
                form.formRegistro(action="/auth/registerSecretaria", method="POST")
                    span.spanRegistro REGISTRAR SECRETARIA
                    fieldset.emailRegistro
                        label.lbEmailRegistro(for="registerEmail") Correo electrónico:
                        input.inputEmailRegistro(type="email", id="registerEmail", name="email", required, placeholder="Correo electrónico")
                    fieldset.pwRegistro
                        label.lbPwRegistro(for="registerPassword") Contraseña:
                        input.inputPwRegistro(type="password", id="registerPassword", name="password", required, placeholder="Contraseña")
                    button.botonRegistro(type="submit") Registrar secretaria
        footer(class="footer")
            section(class="sectionFooter")
                ul Nuestros GitHub
                    li: a(href="https://github.com/JuanCarrillo2435") Carrillo Juan
                    li: a(href="https://github.com/MiguelLuceroSL") Lucero Miguel
                img(src="/img/agenda.png", alt="" class="logoFooter")
                ul Nuestros LinkedIn
                    li: a(href="https://github.com/JuanCarrillo2435") Carrillo Juan
                    li: a(href="https://www.linkedin.com/in/lucero-roque-roberto-miguel-2122bb251/") Lucero Miguel 
        script(src="/js/logout.js")